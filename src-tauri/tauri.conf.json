{
    "$schema": "../node_modules/@tauri-apps/cli/schema.json",
    "app": {
        "security": {
            "csp": null
        },
        "windows": [
            {
                "fullscreen": false,
                "height": 600,
                "minHeight": 600,
                "resizable": true,
                "title": "fformat",
                "width": 800,
                "minWidth": 800
            }
        ]
    },
    "build": {
        "beforeBuildCommand": "pnpm run build",
        "beforeDevCommand": "pnpm run dev",
        "frontendDist": "../out"
    },
    "bundle": {
        "active": true,
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "targets": "all"
    },
    "plugins": {
        "shell": {
            "open": "https://github\\.com/rzmk/fformat"
        }
    },
    "identifier": "com.fformat.dev",
    "productName": "fformat",
    "version": "0.1.0"
}
